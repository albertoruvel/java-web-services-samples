    <version>0.0.1-SNAPSHOT</version>
    <name>ocewsd-samples Maven Webapp</name>
    <url>http://maven.apache.org</url>
    <repositories>
        <repository>
            <id>maven-restlet</id>
            <name>Public online Restlet repository</name>
